{
  "name": "vortex",
  "productName": "Vortex",
  "version": "0.10.0",
  "description": "A tool for listen the music from different providers",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --compilers js:babel-register --recursive --require ./test/setup.js ./test/**/*.test.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "hot-server": "node -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack -p --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack -p --config webpack.config.production.js --progress --profile --colors",
    "loose-envify": "loose-envify ./dist/main.js > ./dist/main.envify.js",
    "optimize": "optimize-js ./dist/main.envify.js > ./dist/main.optimized.js",
    "remove-unused-files": "rm -R ./dist/main.js ./dist/main.envify.js",
    "build": "npm run build-main && npm run build-renderer && npm run loose-envify && npm run optimize && npm run remove-unused-files",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint app test *.js",
    "lint:scss": "stylelint ./app/*.scss",
    "lint:staged": "lint-staged",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./main.development",
    "package": "cross-env NODE_ENV=production node -r babel-register -r babel-polyfill package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.scss": "stylelint"
  },
  "pre-commit": [
    "lint:staged"
  ],
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "author": {
    "name": "Volodimir Partytskyi",
    "email": "volodimir.partytskyi@gmail.com"
  },
  "license": "GNU GPLv2",
  "keywords": [
    "mp3",
    "player",
    "music",
    "audio",
    "video",
    "vk-player"
  ],
  "devDependencies": {
    "asar": "^0.12.3",
    "babel-core": "^6.14.0",
